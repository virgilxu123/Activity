
	body {
		font-family: 'Open Sans', sans-serif;
		line-height: 2em;
		color: #888;
		font-weight: 300;
	}
	#navbar {
		background-color: #52734D;
		font-size:20px;
		position:fixed;
		top: 0;
		left:0px;
		width: 100%;
	}
	#navbar a {
		float: left;
		font-size:16px;
		display: block;
		text-align: center;
		padding: 10px;
		padding-left:20px;
		padding-right:20px;
		text-decoration: none;
		color: #FEFFDE;
	}
	#navbar a:hover {
		background-color: #ddd;
		color: black;
		font-size: 18px;
		border-radius:5px;
	}
	.home {
		margin-left:150px;
		margin-right:50px;
		background-color:#B8F1B0;
		color:#888;
	}
	.container {
		display: grid;
		width: 100%;
		grid-template-columns: 50% 50%;
		padding-top:100px;
	}
	img {
		width:400px;
		height:600px;
		border-radius:60%;
	}
	.image-div > img{
		float:right;
	}
	.image-div {
		padding-right:150px;
		padding-bottom:50px;
	}
	.about-me-div {
		max-width: 600px;
		padding-right:70px;
		padding-top: 100px;
		font-size:20px;
	}
	.aboutme2 {
		background-image:url('Activity/images/container2.jpg');
		border-top:1px groove rgba(0, 0, 0, 0.3);
	}
	.aboutme2 h2{
		text-align:center;
		padding-top:110px;
		padding-bottom:90px;
		font-size:30px;
	}
	.box-container {
		display: grid;
		width: 100%;
		grid-template-columns: auto auto auto;	
		padding-bottom:100px;
	}
	.box h3 {
		font-size:20px;
		text-align:center;
	}
	.box p{
		padding:10px;
		font-size:18px;
		line-height: 1.8em;
	}
	.box1, .box2, .box3 {
		max-width:350px;
		border: 2px groove rgba(0, 0, 0, 0.5);
		background-color:#FEFFDE;
	}
	.box2 {
		margin:auto;
	}
	.box1 {
		float:right;
	}

